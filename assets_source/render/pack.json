{
    pot: true,
    paddingX: 4,
    paddingY: 4,
    bleed: true,
    bleedIterations: 8,
    edgePadding: false,
    duplicatePadding: false,
    rotation: false,
    minWidth: 64,
    minHeight: 64,
    maxWidth: 2048,
    maxHeight: 2048,
    square: true,
    stripWhitespaceX: true,
    stripWhitespaceY: true,
    alphaThreshold: 3,
    filterMin: MipMapLinearLinear,
    filterMag: Nearest,
    wrapX: ClampToEdge,
    wrapY: ClampToEdge,
    format: RGBA8888,
    alias: true,
    outputFormat: png,
    jpegQuality: 1.0,
    ignoreBlankImages: true,
    fast: false,
    debug: false,
    combineSubdirectories: true,
    flattenPaths: true,
    premultiplyAlpha: true,
    useIndexes: true,
    limitMemory: false,
    grid: false,
    scale: [ 1 ],
    scaleSuffix: [ "" ]
}
