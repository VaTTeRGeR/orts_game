{
    pot: true,
    paddingX: 8,
    paddingY: 8,
    bleed: true,
    edgePadding: true,
    duplicatePadding: true,
    rotation: false,
    minWidth: 1024,
    minHeight: 1024,
    maxWidth: 8192,
    maxHeight: 8192,
    square: true,
    stripWhitespaceX: true,
    stripWhitespaceY: true,
    alphaThreshold: 0,
    filterMin: MipMapNearestNearest,
    filterMag: Nearest,
    wrapX: ClampToEdge,
    wrapY: ClampToEdge,
    format: RGBA8888,
    alias: true,
    outputFormat: png,
    jpegQuality: 1.0,
    ignoreBlankImages: true,
    fast: true,
    debug: false,
    combineSubdirectories: true,
    flattenPaths: true,
    premultiplyAlpha: true,
    useIndexes: true,
    limitMemory: false,
    grid: false,
    scale: [ 1 ],
    scaleSuffix: [ "" ]
}